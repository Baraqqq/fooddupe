{
  "name": "@fooddupe/database",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:reset": "dotenv -e .env -- prisma db push --force-reset && dotenv -e .env -- prisma db seed",
    "db:studio": "dotenv -e .env -- prisma studio",
    "db:migrate": "dotenv -e .env -- prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3",
    "dotenv-cli": "^7.4.4",
    "prisma": "^5.7.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "prisma": {
    "seed": "tsx seed.ts"
  }
}
